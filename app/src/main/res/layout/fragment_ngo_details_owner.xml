<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ngoDetailsOwnerFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.01">



        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="Name"
            android:textColor="@color/Dark_blue"
            android:textSize="16dp"
            android:textStyle="bold" />



        <TextView
            android:id="@+id/ngoname"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/shadow_button"
            android:ems="10"
            android:paddingStart="25dp" />




        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="Founder"
            android:textColor="@color/Dark_blue"
            android:textSize="16dp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/ngofounder"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/shadow_button"
            android:ems="10"
            android:paddingStart="25dp" />


        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="Tagline"
            android:textColor="@color/Dark_blue"
            android:textSize="16dp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/ngotagline"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/shadow_button"
            android:ems="10"
            android:paddingStart="25dp" />

        <TextView
            android:id="@+id/textView74"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="Description"
            android:textColor="@color/Dark_blue"
            android:textSize="16dp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/ngodescription"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/shadow_button"
            android:ems="10"
            android:paddingStart="25dp" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="Started on"
            android:textColor="@color/Dark_blue"
            android:textSize="16dp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/ngostart"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/shadow_button"
            android:ems="10"
            android:paddingStart="25dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="Location"
            android:textColor="@color/Dark_blue"
            android:textSize="16dp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/ngolocation"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/shadow_button"
            android:ems="10"
            android:paddingStart="25dp" />

        <TextView
            android:id="@+id/textView85"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="Contact"
            android:textColor="@color/Dark_blue"
            android:textSize="16dp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/ngocontact"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/shadow_button"
            android:ems="10"
            android:paddingStart="25dp" />

        <TextView
            android:id="@+id/textViek"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="Reach out to us"
            android:textColor="@color/Dark_blue"
            android:textSize="16dp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/ngoemail"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/shadow_button"
            android:ems="10"
            android:paddingStart="25dp" />


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>